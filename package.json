{
  "name": "simple-vue-template",
  "displayName": "simple-vue-template",
  "description": "create a simple vue component template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "simple-vue-template.generateVueTemplate",
        "title": "generate vue template"
      },
      {
        "command": "simple-vue-template.generateVueTemplateSimple",
        "title": "Generate Simple Vue Template"
      }
    ],
    "keybindings": [
      {
        "command": "simple-vue-template.generateVueTemplate",
        "key": "ctrl+shift+t",
        "when": "editorTextFocus",
        "mac": "cmd+shift+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
